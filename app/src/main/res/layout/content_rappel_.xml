<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_rappel_"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background3"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.dell.mymobileapplication1.Rappel_Activity"
    tools:showIn="@layout/app_bar_rappel_">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:weightSum="1"
        android:layout_centerHorizontal="true">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="210dp"
            app:srcCompat="@drawable/rap1"
            android:scaleType="fitXY"
            android:cropToPadding="false"
            android:adjustViewBounds="true"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:id="@+id/Prelevement" />


        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView31"
            android:textAllCaps="true"
            android:text="08 Juin"

            android:textSize="30sp"
            android:textColor="@color/Project1Color" />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            app:srcCompat="@color/common_google_signin_btn_text_dark_default"
            android:id="@+id/line" />

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:layout_height="wrap_content"
                    android:id="@+id/textView15"
                    android:textSize="25sp"
                    android:textColor="?android:attr/textColorPrimaryInverse"
                    android:textAlignment="center"
                    android:layout_weight="1.07"
                    android:layout_width="30dp" />

                <TextView
                    android:text="Prise de Tension    08:00 "
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:textSize="18sp"
                    android:fontFamily="sans-serif"
                     />

                <CheckBox
                    android:layout_height="match_parent"
                    android:id="@+id/checkBox2"
                    android:layout_weight="1"
                    android:checked="true"
                    android:layout_width="15dp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView17"
                    android:textAllCaps="true"
                    android:textAlignment="center"
                    android:textSize="14sp"
                    android:textColor="@color/common_google_signin_btn_text_dark_default" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_height="wrap_content"
                    android:id="@+id/textView8"
                    android:textSize="25sp"
                    android:textColor="?android:attr/textColorPrimaryInverse"
                    android:textAlignment="center"
                    android:layout_weight="1.07"
                    android:layout_width="30dp" />

                <TextView
                    android:text="Prise de Glycémie   10:00 "
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:textSize="18sp"
                    android:fontFamily="sans-serif" />

                <CheckBox
                    android:layout_height="match_parent"
                    android:id="@+id/checkBox3"
                    android:layout_weight="1"
                    android:layout_width="15dp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView29"
                    android:textAllCaps="true"
                    android:textAlignment="center"
                    android:textSize="14sp"
                    android:textColor="@color/common_google_signin_btn_text_dark_default" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_height="wrap_content"
                    android:id="@+id/textView28"
                    android:textSize="25sp"
                    android:textColor="?android:attr/textColorPrimaryInverse"
                    android:textAlignment="center"
                    android:layout_weight="1.07"
                    android:layout_width="30dp" />

                <TextView
                    android:text="Prendre Traitement 12:00 "
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"


                    android:textSize="18sp"
                    android:fontFamily="sans-serif"
                     />

                <CheckBox
                    android:layout_height="match_parent"
                    android:id="@+id/checkBox4"
                    android:layout_weight="1"
                    android:layout_width="15dp" />
            </TableRow>

        </TableLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            android:textAllCaps="true"
